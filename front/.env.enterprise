# Iopeer Enterprise Configuration
REACT_APP_NAME=Iopeer Enterprise
REACT_APP_VERSION=2.0.0
REACT_APP_ENV=production
REACT_APP_ENTERPRISE=true

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_API_TIMEOUT=30000

# Features
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_ADVANCED_CACHING=true
REACT_APP_ENABLE_REAL_TIME_METRICS=true

# Performance
REACT_APP_CACHE_TTL=300000
REACT_APP_POLLING_INTERVAL=5000
REACT_APP_MAX_RETRIES=3
REACT_APP_DEBOUNCE_DELAY=500

# Analytics & Monitoring
REACT_APP_ANALYTICS_ENDPOINT=/api/analytics
REACT_APP_ERROR_TRACKING_ENDPOINT=/api/errors
REACT_APP_METRICS_ENDPOINT=/api/metrics
REACT_APP_SESSION_TIMEOUT=3600000

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_RATE_LIMITING=true
REACT_APP_MAX_REQUESTS_PER_MINUTE=100
